{
  "name": "diakinesis-poc",
  "version": "0.1.0",
  "description": "Diakinesis PoC for testing SPAs",
  "main": "index.js",
  "scripts": {
    "serve": "rollup -c",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json nyc --use-spawn-wrap mocha",
    "coverage": "nyc pnpm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bluefinger/diakinesis.git"
  },
  "keywords": [
    "meiosis",
    "reactive",
    "state",
    "state management",
    "routing"
  ],
  "author": "Gon√ßalo Rica Pais da Silva <bluefinger@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bluefinger/diakinesis/issues"
  },
  "homepage": "https://github.com/Bluefinger/diakinesis#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/mergerino": "^0.4.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "rollup": "^2.35.1",
    "rollup-plugin-minify-html-literals": "^1.2.5",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**"
    ],
    "exclude": [
      "benchmarks/**",
      "dist/**",
      "test/**"
    ]
  },
  "dependencies": {
    "lit-html": "^1.3.0",
    "mergerino": "^0.4.0",
    "rythe": "^0.11.0",
    "tslib": "^2.0.3"
  }
}
